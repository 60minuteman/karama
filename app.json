{
  "expo": {
    "name": "Karama",
    "slug": "karama",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.karama.karama",
      "buildNumber": "1.2.0",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "usesAppleSignIn": false,
      "entitlements": {
        "aps-environment": "development"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.karama.karama"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your caregiver."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/Splash.png",
          "resizeMode": "cover",
          "backgroundColor": "#F15858",
          "imageResizeMode": "cover",
          "androidScaleType": "CENTER_CROP",
          "splashScreenBackground": "#F15858",
          "splashScreenAnimation": "fade"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "439c483b-7ef1-4a7b-b7ca-eaa6937c6ed3"
      }
    },
    "runtimeVersion": "1.0.2",
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/439c483b-7ef1-4a7b-b7ca-eaa6937c6ed3",
      "checkAutomatically": "ON_LOAD"
    },
    "owner": "60minute",
    "experiments": {
      "newArchEnabled": true
    }
  }
}
